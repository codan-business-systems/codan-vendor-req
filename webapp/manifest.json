{
	"_version": "1.8.0",
	"sap.app": {
		"id": "req.vendor.codan",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/Z_VENDOR_REQ_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"environmentInfo": {
				"uri": "/sap/opu/odata/sap/z_common_srv",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/env_metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "req.vendor.codan.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "req.vendor.codan.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultOperationMode": "",
					"defaultUpdateMethod": "Put"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "req.vendor.codan.view",
				"controlAggregation": "pages",
				"controlId": "app"
			},
			"routes": [
				{
					"name": "searchVendor",
					"pattern": "",
					"target": [
						"searchVendor"
					]
				},
				{
					"name": "vendorFactSheet",
					"pattern": "vendor/{id}",
					"target": [
						"vendorFactSheet"
					]
				},
				{
					"name": "newVendor",
					"pattern": "new",
					"target": [
						"newVendor"
					]
				}
			],
			"targets": {
				"searchVendor": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "SearchVendor",
					"viewId": "SearchVendor",
					"viewLevel": 1
				},
				"vendorFactSheet": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "VendorFactSheet",
					"viewId": "VendorFactSheet",
					"viewLevel": 1
				},
				"newVendor": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "VendorFactSheet",
					"viewId": "VendorFactSheet",
					"viewLevel": 1
				}
			}
		}
	}
}